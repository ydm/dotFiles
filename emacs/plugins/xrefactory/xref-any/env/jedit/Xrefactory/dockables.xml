<!-- Dockable component definition file for Xrefactory plugin -->

<?xml version="1.0"?>

<!DOCTYPE DOCKABLES SYSTEM "dockables.dtd">

<!-- Xrefactory dockable windows -->

<DOCKABLES>
	<DOCKABLE NAME="xrefactory.browser">
		new com.xrefactory.jedit.DockableBrowser(view, position);
	</DOCKABLE>
	<DOCKABLE NAME="xrefactory.retriever">
		new com.xrefactory.jedit.DockableSymbolRetriever(view, position);
	</DOCKABLE>
	<DOCKABLE NAME="xrefactory.urlviewer">
		new com.xrefactory.jedit.DockableUrlBrowser(view, position);
	</DOCKABLE>
	<DOCKABLE NAME="xrefactory.class-tree-viewer">
		new com.xrefactory.jedit.DockableClassTree(view, position);
	</DOCKABLE>
</DOCKABLES>
